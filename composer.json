{
    "name": "symfonyid/rehat-bundle",
    "type": "symfony-bundle",
    "description": "Provide Crud using HATEOAS Principle",
    "keywords": ["crud", "hateoas", "restful", "rest", "symfony admin", "crud generator", "symfony3", "bundle", "ihsanudin"],
    "homepage": "https://github.com/SymfonyId/RehatBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Komunitas Symfony Indonesia",
            "homepage": "https://github.com/SymfonyId/AdminBundle"
        },
        {
            "name": "Muhammad Surya Ihsanuddin",
            "homepage": "https://github.com/ihsanudin"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "3.0.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "sensio/generator-bundle": "^3.0",
        "willdurand/hateoas-bundle": "^1.1",
        "jms/serializer-bundle": "^1.1",
        "friendsofsymfony/rest-bundle": "^1.7",
        "pagerfanta/pagerfanta": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Symfonian\\Indonesia\\RehatBundle\\": "",
            "Tests\\Symfonian\\Indonesia\\RehatBundle\\": "Tests"
        }
    }
}
